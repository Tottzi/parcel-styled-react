{
  "compilerOptions": {
    "target": "es2016",
    "module": "esnext",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "declaration": true,
    "outDir": "./dist"
  },
  "include": ["src"],
	"exclude": ["node_modules", "dist"]
}